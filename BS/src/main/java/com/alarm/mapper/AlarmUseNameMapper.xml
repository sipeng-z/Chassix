<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alarm.mapper.AlarmUseNameMapper">
    <resultMap id="BaseResultMap" type="com.alarm.domain.AlarmUseName">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="parent_id" jdbcType="INTEGER" property="parentId" />
        <result column="name" jdbcType="VARCHAR" property="name" />
    </resultMap>
    <sql id="Base_Column_List">
    id,parent_id,name
    </sql>
    <select id="findByParentId" resultType="com.alarm.domain.AlarmUseName">
    select
    *
    from dbo.DeviceNameAlarmUse
    where parent_id = #{parentId}
    </select>
</mapper>
