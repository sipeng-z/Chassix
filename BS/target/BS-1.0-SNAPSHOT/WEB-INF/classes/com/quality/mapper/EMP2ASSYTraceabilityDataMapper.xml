  </resultMap>
  <select id="selectBySearch" parameterType="com.quality.model.EMP2ASSYTraceabilityData" resultMap="BaseResultMap">
    select
    ItemNO, Part_Type_NO, MachineNO, SO_Number, Part_Type, PartNO, PartNO_Abbr, PartStatus,
    E1_Press_Force, E1_End_Postion, E1_Final_Force, E2_Press_Force, E2_End_Postion, E2_Final_Force,
    Stud1_Press_Force, Stud1_End_Postion, Stud1_Final_Force, Stud2_Press_Force, Stud2_End_Postion,
    Stud2_Final_Force, Stud3_Press_Force, Stud3_End_Postion, Stud3_Final_Force, E1_E2_Completed_Time,
    Label_Stick_Time, Stud_Assemble_Status, Part_Completed_Time, Last_Cycle_Spent_Time,
    RecordNO, Date_String
    from dbo.EMP2_ASSY_Traceability_Data
    where PartNO LIKE concat(#{partno},'%')
  </select>
  <select id="countBySearch" parameterType="com.quality.model.EMP2ASSYTraceabilityData" resultType="java.lang.Integer">
    select count(*) from dbo.EMP2_ASSY_Traceability_Data
      where PartNO LIKE concat(#{partno},'%')
  </select>
