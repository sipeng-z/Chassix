<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quality.mapper.EMP2ASSYTraceabilityDataMapper">
  <resultMap id="BaseResultMap" type="com.quality.model.EMP2ASSYTraceabilityData">
    <result column="ItemNO" jdbcType="INTEGER" property="itemno" />
    <result column="Part_Type_NO" jdbcType="INTEGER" property="partTypeNo" />
    <result column="MachineNO" jdbcType="CHAR" property="machineno" />
    <result column="SO_Number" jdbcType="CHAR" property="soNumber" />
    <result column="Part_Type" jdbcType="CHAR" property="partType" />
    <result column="PartNO" jdbcType="CHAR" property="partno" />
    <result column="PartNO_Abbr" jdbcType="CHAR" property="partnoAbbr" />
    <result column="PartStatus" jdbcType="INTEGER" property="partstatus" />
    <result column="E1_Press_Force" jdbcType="REAL" property="e1PressForce" />
    <result column="E1_End_Postion" jdbcType="REAL" property="e1EndPostion" />
    <result column="E1_Final_Force" jdbcType="REAL" property="e1FinalForce" />
