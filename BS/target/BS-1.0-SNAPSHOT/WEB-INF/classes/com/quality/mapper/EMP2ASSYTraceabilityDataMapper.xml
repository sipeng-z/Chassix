    <result column="Stud1_Press_Force" jdbcType="REAL" property="stud1PressForce" />
    <result column="Stud1_End_Postion" jdbcType="REAL" property="stud1EndPostion" />
    <result column="Stud1_Final_Force" jdbcType="REAL" property="stud1FinalForce" />
    <result column="Stud2_Press_Force" jdbcType="REAL" property="stud2PressForce" />
    <result column="Stud2_End_Postion" jdbcType="REAL" property="stud2EndPostion" />
